
# HuggingFace API (for MedCPT embeddings)
# Get free API key at: https://huggingface.co/settings/tokens
# IMPORTANT: Set token type to "Read" (not Fine-grained)
# Free tier: 30,000 requests/month
HUGGINGFACE_API_KEY=

# BGE Reranker Configuration
BGE_RERANK_MIN_SCORE=0.3
BGE_RERANK_MAX_LENGTH=512
BGE_RERANK_DEBUG=false

# Semantic Reranking Configuration
SEMANTIC_THRESHOLD=0.65
SEMANTIC_ENABLE_CACHE=true
SEMANTIC_BATCH_SIZE=10

# Database URLs (if using databases)
DATABASE_URL=

# Other existing environment variables...